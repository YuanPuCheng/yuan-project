<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zihui.cwoa.processone.dao.QueryMapper" >

  <select id="queryProStatuByProInstanceId" parameterType="String" resultType="String">
    SELECT NAME_ FROM act_ru_task WHERE PROC_INST_ID_=#{processInstanceId}
  </select>

  <select id="queryCodeByName" parameterType="String" resultType="String">
    SELECT user_code FROM sys_user WHERE user_name=#{userName}
  </select>
</mapper>